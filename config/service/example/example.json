{
  "service": "Example",
  "consoleInfo": {
    "address": "localhost:3000"
  },
  "agentInfo": {
    "name": "Example Agent",
    "ip": "127.0.0.1",
    "port": "5000"
  },
  "node": [
    {
      "name": "Example-Node1",
      "ip": "127.0.0.1",
      "port": "8080",
      "path": "/etc/apache2/uriworkermap.properties",
      "lbMap": [
        {
          "key": "/*",
          "value": "load_balancer"
        }
      ],
      "podList": [
        {
          "name": "Example-Node1-Pod1",
          "lbMap": [
            {
              "key": "/*",
              "value": "load_balancer_ex1"
            }
          ],
          "logPath": "./sample/catalina.out",
          "shPath": "./sample/tomcat1.sh"
        },
        {
          "name": "Example-Node1-Pod2",
          "lbMap": [
            {
              "key": "/*",
              "value": "load_balancer_ex2"
            }
          ],
          "logPath": "./sample/catalina.out",
          "shPath": "./sample/tomcat2.sh"
        }
      ]
    },
    {
      "name": "Example-Node2",
      "ip": "127.0.0.2",
      "port": "8080",
      "path": "/etc/apache2/uriworkermap.properties",
      "lbMap": [
        {
          "key": "/*",
          "value": "load_balancer"
        }
      ],
      "podList": [
        {
          "name": "Example-Node2-Pod1",
          "lbMap": [
            {
              "key": "/*",
              "value": "load_balancer_ex1"
            }
          ],
          "logPath": "./sample/catalina.out",
          "shPath": "./sample/tomcat1.sh"
        },
        {
          "name": "Example-Node2-Pod2",
          "lbMap": [
            {
              "key": "/*",
              "value": "load_balancer_ex2"
            }
          ],
          "logPath": "./sample/catalina.out",
          "shPath": "./sample/tomcat2.sh"
        },
        {
          "name": "Example-Node2-Pod3",
          "lbMap": [
            {
              "key": "/*",
              "value": "load_balancer_ex2"
            }
          ],
          "logPath": "./sample/catalina.out",
          "shPath": "./sample/tomcat3.sh"
        }
      ]
    }
  ]
}
